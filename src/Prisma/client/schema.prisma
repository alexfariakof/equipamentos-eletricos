datasource db {
  provider = "mysql"
  url      = env("MYSQL_CONNECTION_STRING") // Use uma variável de ambiente para a string de conexão
}

model Equipment {
  id        Int      @id @default(autoincrement())
  type      String
  idBarra    Int
  barraDe   Int?
  barraPara Int?
  idLinha Int?
}

// Configuração do gerador para TypeScript
generator client {
  provider        = "prisma-client-js"
  output          = "src/Prisma/client" // ou o caminho correto para o seu projeto
  transpile       = true
  runtimePath     = "@prisma/client/runtime"
}